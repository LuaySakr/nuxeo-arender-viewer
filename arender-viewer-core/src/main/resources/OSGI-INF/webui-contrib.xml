<?xml version="1.0"?>
<component name="org.nuxeo.viewer.arender.webui">
  <require>org.nuxeo.web.ui.resources</require>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="bundles">
    <bundle name="web-ui">
      <resources append="true">
        <resource>webui-arender-bundle.html</resource>
      </resources>
    </bundle>
  </extension>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="resources">
    <resource name="webui-arender-bundle.html" type="import" shrinkable="false">
      <uri>/ui/webui-arender-bundle.html</uri>
    </resource>
  </extension>
</component>
